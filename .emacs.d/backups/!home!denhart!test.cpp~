#include <iostream.h>
using namespace std;
class nonInsulting
{
 private:
  int x,y; 
 public:
  nonInsulting(){
    cout << "Less insulting class names is preferable." << endl;


  };
  virtual ~nonInsulting();
};

int int main(int argc, char *argv[])
{

  nonInsulting denhart;
  denhart.x=1;
      denhart.y=2;
      denhart.nonInsulting;  
  return 0;
}

